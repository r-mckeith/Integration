<%# ASK %>
<%= form_with model: [:user, part], data: {turbo: false} do |f| %>

<%= f.label :name %>
<%= f.text_field :name, autofocus: true %>
<%= f.label :role %>
<%= f.select :role, Part::ROLES, prompt: "Pick one" %>
<%= f.label :description %>
<%= f.text_area :description, rows: 10 %>
<%= f.label :vulnerable %>
<%= f.check_box :vulnerable %>
<%= f.label :protective %>
<%= f.check_box :protective %>
<%= f.label :photo %>
<%= f.text_field :photo %>
<%= f.label :integrated %>
<%= f.check_box :integrated %>
<%= f.submit %>
<% end %>
